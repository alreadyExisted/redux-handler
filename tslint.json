{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "array-type": [
      false
    ],
    "forin": false,
    "arrow-parens": false,
    "callable-types": false,
    "curly": false,
    "member-access": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "max-line-length": [
      false
    ],
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-implicit-dependencies": [
      true,
      ["redux-handler", "rxjs"]
    ],
    "eofline": false,
    "object-literal-sort-keys": false,
    "one-line": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "trailing-comma": [
      false
    ],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "ban-keywords",
      "check-format"
    ],
    "no-var-requires": false,
    "no-submodule-imports": false,
    "semicolon": [
      true,
      "never"
    ],
    "switch-default": false
  }
}