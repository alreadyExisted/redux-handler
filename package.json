{
  "name": "redux-handler",
  "version": "0.10.7",
  "description": "Simple and powerful tool for redux effects for medium and large projects",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "jest": "^22.4.4",
    "npm-run-all": "^4.1.3",
    "redux": "^4.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.11.0",
    "typescript": "3.0.1"
  },
  "peerDependencies": {
    "redux": "^4.0.0",
    "rxjs": "^6.2.2"
  },
  "scripts": {
    "test": "jest",
    "compile": "tsc -p tsconfig.json",
